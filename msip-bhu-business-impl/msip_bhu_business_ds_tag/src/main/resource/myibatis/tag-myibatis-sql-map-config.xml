<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties>
		<property name="dialect" value="mysql" />
	</properties>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="lazyLoadingEnabled" value="false" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="autoMappingBehavior" value="PARTIAL" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="defaultStatementTimeout" value="25000" />
		<setting name="safeRowBoundsEnabled" value="false" />
		<setting name="mapUnderscoreToCamelCase" value="false" />
		<setting name="localCacheScope" value="SESSION" />
		<setting name="jdbcTypeForNull" value="OTHER" />
		<setting name="lazyLoadTriggerMethods" value="equals,clone,hashCode,toString" />
		<setting name="logPrefix" value="mybatisDaoLogger." />
	</settings>
	<typeAliases>
		<typeAlias alias="tagName" type="com.bhu.vas.api.rpc.tag.model.TagName" />
		<typeAlias alias="tagDevices" type="com.bhu.vas.api.rpc.tag.model.TagDevices" />
		<typeAlias alias="tagGroup" type="com.bhu.vas.api.rpc.tag.model.TagGroup" />
		<typeAlias alias="tagGroupRelation"
			type="com.bhu.vas.api.rpc.tag.model.TagGroupRelation" />
		<typeAlias alias="tagGroupHandsetDetail" type="com.bhu.vas.api.rpc.tag.model.TagGroupHandsetDetail" />
	</typeAliases>

	<!-- <plugins> <plugin interceptor="com.google.code.shardbatis.plugin.ShardPlugin"> 
		<property name="shardingConfig" value="myibatis/shard_ucenter_config.xml"/> 
		</plugin> </plugins> -->

	<mappers>
		<mapper resource="com/bhu/vas/api/rpc/tag/model/sqlMapper/TagName.sqlMap.xml" />
		<mapper resource="com/bhu/vas/api/rpc/tag/model/sqlMapper/TagDevices.sqlMap.xml" />
		<mapper resource="com/bhu/vas/api/rpc/tag/model/sqlMapper/TagGroup.sqlMap.xml" />
		<mapper	resource="com/bhu/vas/api/rpc/tag/model/sqlMapper/TagGroupRelation.sqlMap.xml" />
		<mapper	resource="com/bhu/vas/api/rpc/tag/model/sqlMapper/TagGroupHandsetDetail.sqlMap.xml" />
	</mappers>

</configuration>