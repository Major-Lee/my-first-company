<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties>
		<property name="dialect" value="mysql" />
	</properties>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="lazyLoadingEnabled" value="false" />
		<setting name="multipleResultSetsEnabled" value="true" />
		<setting name="useColumnLabel" value="true" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="autoMappingBehavior" value="PARTIAL" />
		<setting name="defaultExecutorType" value="SIMPLE" />
		<setting name="defaultStatementTimeout" value="25000" />
		<setting name="safeRowBoundsEnabled" value="false" />
		<setting name="mapUnderscoreToCamelCase" value="false" />
		<setting name="localCacheScope" value="SESSION" />
		<setting name="jdbcTypeForNull" value="OTHER" />
		<setting name="lazyLoadTriggerMethods" value="equals,clone,hashCode,toString" />
		<setting name="logPrefix" value="mybatisDaoLogger." />
	</settings>
	<typeAliases>
		<typeAlias alias="paymentAlipaylocation" type="com.bhu.vas.api.rpc.payment.model.PaymentAlipaylocation" />
		<typeAlias alias="paymentOrder" type="com.bhu.vas.api.rpc.payment.model.PaymentOrder" />
		<typeAlias alias="paymentReckoning" type="com.bhu.vas.api.rpc.payment.model.PaymentReckoning" />
		<typeAlias alias="paymentWithdraw" type="com.bhu.vas.api.rpc.payment.model.PaymentWithdraw" />
	</typeAliases>

	<mappers>
		<mapper resource="com/bhu/vas/api/rpc/payment/model/sqlMapper/PaymentAlipaylocation.sqlMap.xml" />
		<mapper resource="com/bhu/vas/api/rpc/payment/model/sqlMapper/PaymentOrder.sqlMap.xml" />
		<mapper resource="com/bhu/vas/api/rpc/payment/model/sqlMapper/PaymentReckoning.sqlMap.xml" />
		<mapper resource="com/bhu/vas/api/rpc/payment/model/sqlMapper/PaymentWithdraw.sqlMap.xml" />
	</mappers>

</configuration>