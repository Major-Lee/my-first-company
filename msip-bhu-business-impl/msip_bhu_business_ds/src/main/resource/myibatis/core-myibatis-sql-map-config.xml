<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<properties>
		<property name="dialect" value="mysql" />
	</properties>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="lazyLoadingEnabled" value="false" />
  		<setting name="multipleResultSetsEnabled" value="true"/>
  		<setting name="useColumnLabel" value="true"/>
  		<setting name="useGeneratedKeys" value="false"/>
  		<setting name="autoMappingBehavior" value="PARTIAL"/>
  		<setting name="defaultExecutorType" value="SIMPLE"/>
  		<setting name="defaultStatementTimeout" value="25000"/>
  		<setting name="safeRowBoundsEnabled" value="false"/>
  		<setting name="mapUnderscoreToCamelCase" value="false"/>
  		<setting name="localCacheScope" value="SESSION"/>
  		<setting name="jdbcTypeForNull" value="OTHER"/>
  		<setting name="lazyLoadTriggerMethods" value="equals,clone,hashCode,toString"/>
	</settings>
	<typeAliases>
  		<typeAlias alias="sequence" 			type="com.bhu.vas.api.sequence.model.Sequence"/>
  		<typeAlias alias="user" 				type="com.bhu.vas.api.user.model.User"/>
		<typeAlias alias="userToken" 			type="com.bhu.vas.api.user.model.UserToken"/>
		<typeAlias alias="wifiDeviceCurrentStatus" 			type="com.bhu.vas.api.rpc.devices.model.WifiDeviceCurrentStatus"/>
		<typeAlias alias="wifiDevice" 			type="com.bhu.vas.api.rpc.devices.model.WifiDevice"/>
		<typeAlias alias="wifiDeviceGroup" 			type="com.bhu.vas.api.rpc.devices.model.WifiDeviceGroup"/>
		<typeAlias alias="wifiDeviceAlarm" 			type="com.bhu.vas.api.rpc.devices.model.WifiDeviceAlarm"/>
		<typeAlias alias="handsetDevice" 			type="com.bhu.vas.api.rpc.devices.model.HandsetDevice"/>
		<typeAlias alias="wifiDeviceCompletedDownTask" 			type="com.bhu.vas.api.rpc.task.model.WifiDeviceCompletedDownTask"/>
		<typeAlias alias="wifiDeviceDownTask" 			type="com.bhu.vas.api.rpc.task.model.WifiDeviceDownTask"/>
	</typeAliases>
	
	<!-- 
	<plugins>
        <plugin interceptor="com.google.code.shardbatis.plugin.ShardPlugin">
                <property name="shardingConfig" value="myibatis/shard_ucenter_config.xml"/>
        </plugin>
	</plugins>
	 -->
	
	<mappers>
		 
		<mapper resource="com/bhu/vas/api/sequence/model/sqlMapper/Sequence.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/user/model/sqlMapper/User.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/user/model/sqlMapper/UserToken.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/rpc/devices/model/sqlMapper/WifiDeviceCurrentStatus.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/rpc/devices/model/sqlMapper/WifiDevice.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/rpc/devices/model/sqlMapper/WifiDeviceGroup.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/rpc/devices/model/sqlMapper/HandsetDevice.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/rpc/task/model/sqlMapper/WifiDeviceCompletedDownTask.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/rpc/task/model/sqlMapper/WifiDeviceDownTask.sqlMap.xml"/>
		<mapper resource="com/bhu/vas/api/rpc/devices/model/sqlMapper/WifiDeviceAlarm.sqlMap.xml"/>
	</mappers>
	
</configuration>